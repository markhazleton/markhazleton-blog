{
    "title": "Resume",
    "name": "markhazleton-resume",
    "version": "8.0.1",
    "scripts": {
        "build": "npm run clean && npm run build:pug && npm run build:scss && npm run build:scripts && npm run build:assets",
        "build:assets": "node scripts/build-assets.js",
        "build:pug": "node scripts/build-pug.js",
        "build:scripts": "node scripts/build-scripts.js",
        "build:scss": "node scripts/build-scss.js",
        "clean": "node scripts/clean.js",
        "start": "npm run build && node scripts/start.js",
        "start:debug": "npm run build && node scripts/start-debug.js"
    },
    "description": "Resume is a simple, yet elegant resume and CV theme for Bootstrap 4.",
    "keywords": [
        "css",
        "sass",
        "html",
        "responsive",
        "theme",
        "template"
    ],
    "homepage": "https://github.com/MarkHazleton/markhazleton-resume",
    "bugs": {
        "url": "https://github.com/MarkHazleton/markhazleton-resume/issues",
        "email": "mark.hazleton@controlorigins.com"
    },
    "license": "MIT",
    "author": "Start Bootstrap",
    "contributors": [
        "David Miller (https://davidmiller.io/)",
        "Mark Hazleton (https://markhazleton.controlorigins.com)"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/MarkHazleton/markhazleton-resume.git"
    },
    "dependencies": {
        "bootstrap": "5.3.1",
        "g": "^2.0.1",
        "npm-check-updates": "^16.10.17"
    },
    "devDependencies": {
        "autoprefixer": "10.4.14",
        "browser-sync": "^2.29.3",
        "chokidar": "3.5.3",
        "concurrently": "8.2.0",
        "postcss": "8.4.27",
        "prettier": "3.0.0",
        "pug": "3.0.2",
        "sass": "1.64.1",
        "shelljs": "0.8.5",
        "upath": "2.0.1"
    }
}
